{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {
        "@ionic/angular-toolkit:page": {
          "styleext": "scss",
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "www"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "src/global.scss",
              "./node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": [
              "./node_modules/leaflet/dist/leaflet.js"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1000kb",
                  "maximumError": "1500kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "analyze-bundle": {
              "sourceMap": true,
              "outputHashing": "none",
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "ci": {
              "progress": false
            },
            "android-dev": {
              "fileReplacements": [
                {
                  "replace": "src/app/services/file/file.service.ts",
                  "with": "capacitor/services/file/file.service.ts"
                },
                {
                  "replace": "src/app/services/geolocation/geolocation.service.ts",
                  "with": "capacitor/services/geolocation/geolocation.service.ts"
                },
                {
                  "replace": "src/app/services/http/http-client.service.ts",
                  "with": "capacitor/services/http/http-client.service.ts"
                },
                {
                  "replace": "src/app/services/network/network.service.ts",
                  "with": "capacitor/services/network/network.service.ts"
                },
                {
                  "replace": "src/app/services/trailence.service.ts",
                  "with": "capacitor/services/trailence.service.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "capacitor/environments/android.dev.ts"
                }
              ]
            },
            "android-local": {
              "fileReplacements": [
                {
                  "replace": "src/app/services/file/file.service.ts",
                  "with": "capacitor/services/file/file.service.ts"
                },
                {
                  "replace": "src/app/services/geolocation/geolocation.service.ts",
                  "with": "capacitor/services/geolocation/geolocation.service.ts"
                },
                {
                  "replace": "src/app/services/http/http-client.service.ts",
                  "with": "capacitor/services/http/http-client.service.ts"
                },
                {
                  "replace": "src/app/services/network/network.service.ts",
                  "with": "capacitor/services/network/network.service.ts"
                },
                {
                  "replace": "src/app/services/trailence.service.ts",
                  "with": "capacitor/services/trailence.service.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "capacitor/environments/android.local.ts"
                }
              ]
            },
            "android-prod": {
              "fileReplacements": [
                {
                  "replace": "src/app/services/file/file.service.ts",
                  "with": "capacitor/services/file/file.service.ts"
                },
                {
                  "replace": "src/app/services/geolocation/geolocation.service.ts",
                  "with": "capacitor/services/geolocation/geolocation.service.ts"
                },
                {
                  "replace": "src/app/services/http/http-client.service.ts",
                  "with": "capacitor/services/http/http-client.service.ts"
                },
                {
                  "replace": "src/app/services/network/network.service.ts",
                  "with": "capacitor/services/network/network.service.ts"
                },
                {
                  "replace": "src/app/services/trailence.service.ts",
                  "with": "capacitor/services/trailence.service.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "capacitor/environments/android.prod.ts"
                }
              ]
            }
          },
        "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "development": {
              "buildTarget": "app:build:development"
            },
            "ci": {
              "progress": false
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "test/assets",
                "output": "assets/test"
              }
            ],
            "styles": [
              "src/global.scss",
              "./node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": [
              "./node_modules/leaflet/dist/leaflet.js"
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false,
              "codeCoverage": true,
              "browsers": "ChromeHeadless"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@ionic/angular-toolkit"
    ],
    "analytics": false
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
