{
  "name": "trailence",
  "version": "0.10.1",
  "author": "Guillaume Le Cousin",
  "homepage": "https://www.trailence.org/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "install-global-commands": "npm install -g cpy-cli rimraf mkdirp",
    "update-android": "ionic capacitor update android",
    "install-android": "ionic capacitor copy android --configuration=android-%npm_config_env% && rimraf android/app/src/main/assets/public/assets/apk && cd android/environments/%npm_config_env% && cpy . ../..",
    "install-android-dev": "npm run install-android --env=dev",
    "install-android-local": "npm run install-android --env=local",
    "install-android-prod": "npm run install-android --env=prod",
    "set-version": "node ./scripts/set-version.js %npm_config_newversion%",
    "check-i18n": "node ./scripts/check-i18n-keys.js",
    "check-assets-version": "node ./scripts/check-assets-version.js",
    "release": "npm run check-i18n && npm run check-assets-version && npm run set-version && ng build --configuration=production",
    "instrument": "nyc instrument www/browser www/browser --exclude-after-remap=false --complete-copy --in-place --exclude=**/polyfills*.js --exclude=**/scripts*.js --exclude=**/swipe-back*.js --exclude=**/shadow-css*.js --exclude=**/jszip*.js --exclude=**/ios.transition*.js --exclude=**/md.transition*.js --exclude=**/input-shims*.js --exclude=**/hardward-back-button*.js --exclude=**/focus-visible*.js --exclude=**/keyboard2*.js --exclude=**/status-tap*.js"
  },
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/common": "^19.0.3",
    "@angular/compiler": "^19.0.3",
    "@angular/core": "^19.0.3",
    "@angular/forms": "^19.0.3",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/router": "^19.0.3",
    "@capacitor-community/background-geolocation": "^1.2.19",
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "^6.0.2",
    "@capacitor/core": "^6.2.0",
    "@capacitor/geolocation": "^6.1.0",
    "@capacitor/haptics": "^6.0.2",
    "@capacitor/keyboard": "^6.0.3",
    "@capacitor/network": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@capawesome/capacitor-file-picker": "^6.2.0",
    "@ionic/angular": "^8.4.1",
    "angular-code-input": "^2.0.0",
    "chart.js": "^4.4.7",
    "dexie": "^4.0.10",
    "immutable": "^5.0.3",
    "ionicons": "^7.4.0",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "ng2-charts": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.7.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.4",
    "@angular-eslint/builder": "^19.0.1",
    "@angular-eslint/eslint-plugin": "^19.0.1",
    "@angular-eslint/eslint-plugin-template": "^19.0.1",
    "@angular-eslint/schematics": "^19.0.1",
    "@angular-eslint/template-parser": "^19.0.1",
    "@angular/cdk": "^19.0.2",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.3",
    "@angular/language-service": "^19.0.3",
    "@capacitor/cli": "^6.2.0",
    "@ionic/angular-toolkit": "^12.1.1",
    "@ionic/cli": "^7.2.0",
    "@types/jasmine": "^5.1.5",
    "@types/leaflet": "^1.9.15",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "jasmine-core": "^5.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nyc": "^17.1.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3"
  },
  "description": "Trailence"
}
