{
  "name": "trailence",
  "version": "0.19.7",
  "author": "Guillaume Le Cousin",
  "homepage": "https://www.trailence.org/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-ci": "ng test --configuration=ci --watch=false --no-watch",
    "lint": "ng lint",
    "install-global-commands": "npm install -g cpy-cli rimraf mkdirp",
    "update-android": "ionic capacitor update android",
    "install-android": "ionic capacitor copy android --configuration=android-%npm_config_env% && rimraf android/app/src/main/assets/public/assets/apk && rimraf android/app/src/main/assets/public/assets/admin && rimraf android/app/src/main/assets/public/assets/home-page && cd android/environments/%npm_config_env% && cpy . ../..",
    "install-android-dev": "npm run install-android --env=dev",
    "install-android-local": "npm run install-android --env=local",
    "install-android-prod": "npm run install-android --env=prod",
    "install-android-playstore": "npm run install-android --env=playstore",
    "install-android-appgallery": "npm run install-android --env=appgallery",
    "set-version": "node ./scripts/set-version.js %npm_config_newversion%",
    "check-i18n": "node ./scripts/check-i18n-keys.js",
    "check-assets-version": "node ./scripts/check-assets-version.js",
    "check-release-note": "node ./scripts/check-release-note.js %npm_config_newversion%",
    "release": "npm run check-i18n && npm run check-assets-version && npm run check-release-note && npm run set-version && ng build --configuration=production",
    "instrument": "nyc instrument www/browser www/browser --exclude-after-remap=false --complete-copy --in-place --exclude=**/polyfills*.js --exclude=**/scripts*.js --exclude=**/swipe-back*.js --exclude=**/shadow-css*.js --exclude=**/jszip*.js --exclude=**/ios.transition*.js --exclude=**/md.transition*.js --exclude=**/input-shims*.js --exclude=**/hardward-back-button*.js --exclude=**/focus-visible*.js --exclude=**/keyboard2*.js --exclude=**/status-tap*.js",
    "analyze-bundle": "ng build --configuration=analyze-bundle --progress=false --stats-json > build.output && node ./scripts/extract-initial-bundle.js ./build.output",
    "generate-public-pages": "cd scripts && node --import=tsx generate-public-pages.ts && cd ..",
    "generate-local-nginx": "cd docker/local && node --import=tsx generate-local-nginx.ts && cd ../..",
    "make-leaflet-esm": "node ./scripts/make-leaflet-esm.js",
    "postinstall": "node ./scripts/make-leaflet-esm.js && node ./scripts/fix-ng2-charts.js",
    "build-assets": "node --import=tsx src/icons/mapping/build.ts"
  },
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.1.7",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-browser-dynamic": "^20.1.7",
    "@angular/router": "^20.1.7",
    "@capacitor-community/background-geolocation": "^1.2.22",
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.2",
    "@capacitor/core": "^6.2.1",
    "@capacitor/geolocation": "^6.1.0",
    "@capacitor/haptics": "^6.0.2",
    "@capacitor/keyboard": "^6.0.3",
    "@capacitor/network": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@capawesome/capacitor-file-picker": "^6.2.0",
    "@ionic/angular": "^8.7.2",
    "angular-code-input": "^2.0.0",
    "chart.js": "^4.5.0",
    "dexie": "^4.0.11",
    "immutable": "^5.1.3",
    "ionicons": "^8.0.13",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.7.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.7",
    "@angular/language-service": "^20.1.7",
    "@capacitor/cli": "^6.2.1",
    "@ionic/angular-toolkit": "^12.3.0",
    "@ionic/cli": "^7.2.1",
    "@types/jasmine": "^5.1.8",
    "@types/leaflet": "^1.9.20",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "jasmine-core": "^5.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nyc": "^17.1.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "description": "Trailence"
}
