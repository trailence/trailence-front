    "@ionic/angular": "^8.2.7",
    "angular-code-input": "^2.0.0",
    "chart.js": "^4.4.4",
    "dexie": "^4.0.8",
    "ionicons": "^7.4.0",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "ng2-charts": "^6.0.1",
