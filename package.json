{
  "name": "trailence",
  "version": "0.1.0",
  "author": "Guillaume Le Cousin",
  "homepage": "https://www.trailence.org/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "install-global-commands": "npm i cpy-cli -g",
    "update-android": "ionic capacitor update android",
    "install-android": "ionic capacitor copy android --configuration=android-%npm_config_env% && cd android/environments/%npm_config_env% && cpy . ../../app/src/main",
    "install-android-dev": "npm run install-android --env=dev",
    "install-android-local": "npm run install-android --env=local",
    "set-version": "node ./scripts/set-version.js %npm_config_version%"
  },
  "license": "Apache-2.0",
  "private": true,
