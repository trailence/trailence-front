    "dexie": "4.0.4",
    "ionicons": "^7.4.0",
    "leaflet": "^1.9.4",
    "ng2-charts": "^6.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.7"
  },
