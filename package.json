    "@angular/core": "^18.0.6",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.0.6",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@capacitor-community/background-geolocation": "^1.2.17",
    "@capacitor/android": "^6.1.0",
